{
    "extends": [
        "airbnb",
        "prettier"
    ],
    "env": {
        "jest": true
    },
    "globals": {
        "document": true
    },
    "rules": {
        "react/prop-types": "warn",
        "import/no-unresolved": 1,
        "react/jsx-props-no-spreading": 0,
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js"
                ]
            }
        ]
        
       
    },
    "settings": {
        "import/resolver": {
            "node": {
                "paths": [
                    "src"
                ]
            }
        }
    }
}